{
  "name": "brandplan",
  "version": "0.1.0",
  "description": "BrandPlan CLI for scaffolding and building brand token CSS",
  "type": "module",
  "bin": {
    "brandplan": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@brandplan/core": "workspace:*",
    "commander": "^12.1.0",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.9"
  }
}
